# API Keys (rename to .env and fill in your keys)

# Alpha Vantage API Key (free tier: 5 requests/min, 500 requests/day)
# Get your key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Polygon.io API Key (optional, for real-time data)
# Get your key at: https://polygon.io/
POLYGON_API_KEY=your_polygon_key_here

# News API Key (optional, for sentiment analysis)
# Get your key at: https://newsapi.org/
NEWS_API_KEY=your_news_api_key_here

# Database Configuration (if using PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=stocks

# Redis Configuration (for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# MLflow Tracking Server
MLFLOW_TRACKING_URI=http://localhost:5000

# Environment
ENVIRONMENT=development  # development, production
DEBUG=true
